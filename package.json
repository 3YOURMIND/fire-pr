{
	"name": "fire-pr-extension",
	"version": "0.0.6",
	"description": "The official fire-pr extension for Chrome",
	"author": "James <jo@3yourmind.com>",
	"main": "./fire-pr-core/dist/fire-pr-core.cjs.js",
	"module": "./fire-pr-core/dist/fire-pr-core.esm.js",
	"browser": "./fire-pr-core/dist/fire-pr-core.js",
	"scripts": {
		"precommit": "lint-staged",
		"test": "jest",
		"web:build":
			"cross-env NODE_ENV=production webpack --config ./fire-pr-web/webpack.config.js --progress --hide-modules && node ./fire-pr-web/scripts/build-zip.js",
		"web:dev":
			"cross-env NODE_ENV=development webpack --config ./fire-pr-web/webpack.config.js --progress --hide-modules",
		"web:storybook": "start-storybook -p 6006",
		"web:build-storybook": "build-storybook",
		"core:run-example": "node ./fire-pr-core/example.js",
		"core:build": "rollup -c ./rollup.config.js",
		"site:dev": "nuxt",
		"site:build": "nuxt build",
		"site:start": "nuxt start",
		"site:generate": "nuxt generate"
	},
	"lint-staged": {
		"*.{js,json,css,md,vue}": ["prettier --write", "git add"]
	},
	"dependencies": {
		"@nuxtjs/pwa": "^2.0.8",
		"nuxt": "^1.4.0",
		"purecss": "^1.0.0",
		"vue": "^2.5.16",
		"vue-router": "^3.0.1",
		"vuex": "^3.0.1"
	},
	"devDependencies": {
		"@storybook/addon-actions": "^3.4.0",
		"@storybook/addon-links": "^3.4.0",
		"@storybook/addons": "^3.4.0",
		"@storybook/vue": "^3.4.0",
		"@vue/devtools": "^1.1.0",
		"babel-core": "^6.26.0",
		"babel-jest": "^22.4.3",
		"babel-loader": "^7.1.4",
		"babel-preset-env": "^1.6.1",
		"babel-preset-latest": "^6.24.1",
		"babel-preset-vue": "^2.0.2",
		"copy-webpack-plugin": "^4.5.1",
		"cross-env": "^5.1.4",
		"css-loader": "^0.28.11",
		"eslint-config-prettier": "^2.9.0",
		"eslint-plugin-prettier": "^2.6.0",
		"eslint-plugin-vue": "^4.4.0",
		"extract-text-webpack-plugin": "^3.0.2",
		"file-loader": "^1.1.11",
		"husky": "^0.14.3",
		"jest": "^22.4.3",
		"lint-staged": "^7.0.3",
		"node-sass": "^4.8.3",
		"postcss-loader": "^2.1.3",
		"prettier": "^1.11.1",
		"rollup": "^0.57.1",
		"rollup-plugin-uglify": "^3.0.0",
		"sass-loader": "^6.0.7",
		"storybook-router": "^0.3.4",
		"vue-loader": "^14.2.2",
		"vue-style-loader": "^4.1.0",
		"vue-template-compiler": "^2.5.16",
		"webpack": "^3.11.0",
		"webpack-shell-plugin": "^0.5.0",
		"zip-folder": "^1.0.0"
	}
}
