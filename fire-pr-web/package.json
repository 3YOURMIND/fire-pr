{
	"name": "fire-pr-extension",
	"version": "0.0.3",
	"description": "The official fire-pr extension for Chrome",
	"author": "James <jo@3yourmind.com>",
	"scripts": {
		"build":
			"cross-env NODE_ENV=production webpack --config webpack.config.js --progress --hide-modules",
		"build:dev":
			"cross-env NODE_ENV=development webpack --config webpack.config.js --progress --hide-modules",
		"build-zip": "node scripts/build-zip.js",
		"watch": "npm run build -- --watch",
		"watch:dev": "npm run build:dev -- --watch",
		"precommit": "lint-staged",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"lint-staged": {
		"*.{js,json,css,md,vue}": ["prettier --write", "git add"]
	},
	"dependencies": {
		"purecss": "^1.0.0",
		"vue": "^2.5.13",
		"vue-router": "^3.0.1",
		"vuex": "^3.0.1"
	},
	"devDependencies": {
		"@storybook/addon-actions": "^3.3.14",
		"@storybook/addon-links": "^3.3.14",
		"@storybook/addons": "^3.3.14",
		"@storybook/vue": "^3.3.14",
		"@vue/devtools": "^1.0.2",
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.3",
		"babel-preset-latest": "^6.24.1",
		"babel-preset-vue": "^2.0.1",
		"copy-webpack-plugin": "^4.4.2",
		"cross-env": "^5.1.3",
		"css-loader": "^0.28.10",
		"eslint-config-prettier": "^2.9.0",
		"eslint-plugin-prettier": "^2.6.0",
		"eslint-plugin-vue": "^4.3.0",
		"extract-text-webpack-plugin": "^3.0.2",
		"file-loader": "^1.1.10",
		"husky": "^0.14.3",
		"lint-staged": "^7.0.0",
		"node-sass": "^4.7.2",
		"prettier": "1.11.0",
		"sass-loader": "^6.0.6",
		"storybook-router": "^0.3.2",
		"vue-loader": "^14.1.1",
		"vue-style-loader": "^4.0.2",
		"vue-template-compiler": "^2.5.13",
		"webpack": "^3.11.0",
		"webpack-shell-plugin": "^0.5.0",
		"zip-folder": "^1.0.0"
	}
}
